import fileinput

for line in fileinput.input():
    st = line.split(' ')
    n, a, r = int(st[0]), int(st[1]), 0
    for i in xrange(1 << n, a + 1):
        m, b = n, i
        while b > 0 and m >= 0:
            if b % 2 == 0:
                m -= 1
            b >>= 1
        if m == 0:
            r += 1
    print r
